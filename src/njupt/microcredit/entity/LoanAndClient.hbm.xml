<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="njupt.microcredit.entity">
	<class name="LoanAndClient" table="loan_and_client">
	    <id name="loanClientId" column="LOAN_CLIENT_ID">
	    	<generator class="native"></generator>
	    </id>
		<property name="loanType" column="LOAN_TYPE"></property>
		<property name="loanClass" column="LOAN_CLASS"></property>
		<property name="guranteeMethod" column="GURANTEE_METHOD"></property>
		<property name="loanUse" column="LOAN_USE"></property>
		<property name="appliAmount" column="APPLI_AMOUNT"></property>
		<property name="loanTerm" column="LOAN_TERM"></property>
		<property name="maxInstallment" column="MAX_INSTALLMENT"></property>
		<property name="appliReason" column="APPLI_REASON"></property>	
		<property name="clientInfoId" column="CLIENT_INFO_ID"></property>	
		<property name="clientName" column="CLIENT_NAME"></property>
		<property name="clientGender" column="CLIENT_GENDER"></property>
		<property name="clientAge" column="CLIENT_AGE"></property>			
		<property name="clientBirthday" column="CLIENT_BIRTHDAAY" length="10"></property>
		<property name="phoneNumber" column="PHONE_NUMBER"></property>	
		<property name="clientEmail" column="CLIENT_EMAIL"></property>
		<property name="clientCensus" column="CLIENT_CENSUS"></property>
		<property name="clientEducation" column="CLIENT_EDUCATION"></property>
		<property name="maritalStatus" column="MARITALSTATUS"></property>
		<property name="clentAddress" column="CLIENT_ADDRESS"></property>	
		<property name="addrYears" column="ADD_TEARS"></property>	
		<property name="identifyType" column="IDENTIFY_TYPE"></property>	
		<property name="identifyNum" column="IDENTIFY_NUM"></property>	
	</class>

</hibernate-mapping>