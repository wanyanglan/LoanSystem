<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="njupt.microcredit.entity">
	<class name="ClientInfo" table="client_info">
	    <id name="clientId" column="CLIENT_ID">
	    	<generator class="native"></generator>
	    </id>
		<property name="clientName" column="CLIENT_NAME"></property>
		<property name="clientGender" column="CLIENT_GENDER"></property>
		<property name="clientAge" column="CLIENT_AGE"></property>
		<property name="phoneNumber" column="PHONE_NUMBER"></property>
		<property name="identifyType" column="IDENTIFY_TYPE"></property>
		<property name="identifyNum" column="IDENTIFY_NUM"></property>
		<property name="maritalStatus" column="MARITAL_STATUS"></property>
		<property name="clientEducation" column="CLIENT_EDUCATION"></property>
		<property name="clientCensus" column="CLIENT_CENSUS"></property>
		<property name="clentAddress" column="CLIENT_ADDRESS"></property>
		<property name="clientBirthday" column="CLIENT_BIRTHDAY"></property>
		<property name="addrYears" column="ADDR_YEARS"></property>
		<property name="jabTitle" column="JOB_TITLE"></property>
		<property name="occupationTitle" column="OCCUPATION_TITLE"></property>
		<property name="monthlyIncome" column="MONTHLY_INCOME"></property>
		<property name="unitService" column="UNIT_SERVICE"></property>
		<property name="clientEmail" column="CLIENT_EMAIL"></property>
		<property name="badRecord" column="BAD_RECORD"></property>
		<property name="unitType" column="UNIT_TYPE"></property>
		<property name="companyAddress" column="COMPANY_ADDRESS"></property>
		<property name="companyPhone" column="COMPANY_PHONE"></property>
		<property name="companyName" column="COMPANY_NAME"></property>
		<property name="updateDate" column="UPDATE_DATE"></property>
		<property name="createUser" column="CREATE_USER"></property>
		<property name="clientBelong" column="CLIENTBELONG"></property>	
	</class>

</hibernate-mapping>