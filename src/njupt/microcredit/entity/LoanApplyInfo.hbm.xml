<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="njupt.microcredit.entity">
	<class name="LoanApplyInfo" table="loan_apply_info">
	    <id name="loanId" column="LOAN_ID">
	    	<generator class="native"></generator>
	    </id>
		<property name="loanType" column="LOAN_TYPE"></property>
		<property name="loanClass" column="LOAN_CLASS"></property>
		<property name="loanUse" column="LOAN_USE"></property>
		<property name="applyAmount" column="APPLY_AMOUNT"></property>
