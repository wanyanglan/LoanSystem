		<property name="guranteeMethod" column="GURANTEE_METHOD"></property>
		<property name="applyTime" column="APPLY_TIME"></property>
		<property name="appliReason" column="APPLY_REASON"></property>
		<property name="loanCreditDate" column="LOAN_CREDIT_DATE"></property>
		<property name="loanStage" column="LOAN_STAGE"></property>
		<property name="loanRefuseReason" column="LOAN_REFUSE_REASON"></property>
		<property name="loanRefuseRemark" column="LOAN_REFUSE_REMARK"></property>
		<property name="loanRefuseDate" column="LOAN_REFUSE_DATE"></property>
		<property name="loanRefuseUser" column="LOAN_REFUSE_USER"></property>
		<property name="loanCollectionState" column="LOAN_COLLECTION_STATE"></property>
		<property name="loanCollectionDate" column="LOAN_COLLECTION_DATE"></property>
		<property name="loanRepayAmount" column="LOAN_REPAY_AMOUNT"></property>
		<property name="loanRepayDate" column="LOAN_REPAY_DATE"></property>
		<property name="loanBelongId" column="LOAN_BELONG_ID"></property>
		<property name="repaymentMode" column="REPAYMENT_MODE"></property>
		<property name="isDel" column="IS_DEL"></property>
		<property name="loanAccountNo" column="LOAN_ACCOUNT_NO"></property>
		<property name="loanAccountAmount" column="LOAN_ACCOUNT_AMOUNT"></property>
		<property name="loanBalanceAmount" column="LOAN_BALANCE_AMOUNT"></property>
		<property name="loanProposeAmount" column="LOAN_PROPOSE_AMOUNT"></property>
		<property name="loanCreditAmount" column="LOAN_CREDIT_AMOUNT"></property>
		<property name="loanContractNumber" column="LOAN_CONTRACT_NUMBER"></property>
		<property name="custName" column="CUST_NAME"></property>
		<property name="custType" column="CUST_TYPE"></property>
		<property name="custId" column="CUST_ID"></property>
		<property name="identifyType" column="IDENTIFY_TYPE"></property>
		<property name="identifyNum" column="IDENTIFY_NUM"></property>
		<property name="phoneNum" column="PHONE_NUM"></property>
		<property name="loanAuditDate" column="LOAN_AUDIT_DATE"></property>
		<property name="pledgeInfoId" column="PLEDGE_INFO_ID"></property>
		<property name="updateDate" column="UPDATE_DATE"></property>
		<property name="createUser" column="CREATE_USER"></property>
	</class>

</hibernate-mapping>